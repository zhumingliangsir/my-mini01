<!--index.wxml-->
<text class="title">{{title}}</text>

<view wx:for="{{arr}}" wx:key="itme.name">{{item.name}} ^_^ {{item.age}}</view>

<view>{{count}}</view>
<button size="mini" bindtap="add">点击增加</button>
<button size="mini" bindtap="reduce">点击减少</button>
<button size="mini" bindtap="reset">重置</button>
